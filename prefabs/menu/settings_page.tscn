code = "shader_type canvas_item;

void fragment() {
	vec2 muv = (UV - vec2(0.5)) * 2.0;
	float m = 1.0 - max(abs(muv.x),abs(muv.y));
	COLOR = texture(TEXTURE,UV)*(1.0 + clamp((m*1.5+0.5),0,1.0))/2.0;
}"

