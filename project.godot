[application]

config/name="Sound Space+"
run/main_scene="res://init.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SoundSpacePlus"
boot_splash/image="res://splash.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"
config/version="0.6.0-alpha"
config/autoload_map_registries="test maps|res://test_assets/test_maps.sspmr;built in|res://content/songs/built_in_maps.sspmr"

[audio]

channel_disable_threshold_db=-240.0
channel_disable_time=15.0

[autoload]

Globals="*res://classes/Globals.gd"
